{
	"cli": {
		"version": ">= 15.0.15",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"channel": "development",
			"android": {
				"buildType": "apk"
			}
		},
		"preview": {
			"distribution": "internal",
			"channel": "preview",
			"android": {
				"buildType": "apk"
			}
		},
		"production": {
			"autoIncrement": true,
			"channel": "production",
			"android": {
				"buildType": "app-bundle",
				"resourceClass": "medium"
			},
			"ios": {
				"resourceClass": "medium"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
